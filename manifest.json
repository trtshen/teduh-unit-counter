{
  "manifest_version": 3,
  "name": "Teduh: Unit Counter",
  "version": "1.0",
  "description": "Counts units status on the page.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.webp",
      "48": "icon.webp",
      "128": "icon.webp"
    }
  }
}