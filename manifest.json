{
  "manifest_version": 3,
  "name": "Teduh: Unit Counter",
  "version": "1.1.3",
  "description": "Counts units status on the page.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "house-icon.png",
      "48": "house-icon.png",
      "128": "house-icon.png"
    }
  }
}